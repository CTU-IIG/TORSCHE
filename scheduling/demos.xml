<demos>
	<name>TORSCHE Scheduling Toolbox for Matlab</name>
	<type>toolbox</type>
	<icon>$toolbox/matlab/icons/matlabicon.gif</icon>
	<description isCdata="no">
		<p>TORSCHE Scheduling Toolbox for Matlab demos.</p>
	</description>
	
	<demosection>
		<label>Scheduling Algorithms</label>
		<demoitem>
			<label>Horn algorithm</label>
			<callback>horn_demo</callback>
		</demoitem>
		<demoitem>
			<label>Scheduling problem '1|rj|Cmax'</label>
			<callback>alg1rjcmax_demo</callback>
		</demoitem>
		<demoitem>
			<label>Scheduling problem '1|rj,~dj|Cmax'</label>
			<callback>bratley_demo</callback>
		</demoitem>
		<demoitem>
			<label>Scheduling problem '1||sumUj'</label>
			<callback>alg1sumuj_demo</callback>
		</demoitem>
		<demoitem>
			<label>Scheduling problem 'P||Cmax'</label>
			<callback>algpcmax_demo</callback>
		</demoitem>
		<demoitem>
			<label>Scheduling problem 'P|pmtn|Cmax'</label>
			<callback>mcnaughtonrule_demo</callback>
		</demoitem>
		<demoitem>
			<label>Scheduling problem 'P2|prec,pj=1|Cmax'</label>
			<callback>coffmangraham_demo</callback>
		</demoitem>
		<demoitem>
			<label>Scheduling problem 'P|in-tree,pj=1|Cmax'</label>
			<callback>hu_demo</callback>
		</demoitem>
		<demoitem>
			<label>Scheduling problem 'P|rj,~dj,prec|Cmax'</label>
			<callback>algprjdeadlinepreccmax_demo</callback>
		</demoitem>
		<demoitem>
			<label>List Scheduling and Strategy</label>
			<callback>listsch_demo</callback>
		</demoitem>
	</demosection>
	
	<demosection>
		<label>Cyclic Scheduling</label>
		<demoitem>
			<label>Cyclic Scheduling</label>
			<callback>cycsch_demo</callback>
		</demoitem>
		<demoitem>
			<label>Cyclic Scheduling (CSSIM)</label>
			<callback>cssim_demo</callback>
		</demoitem>
		<demoitem>
			<label>Cyclic Scheduling - integer/binary method</label>
			<callback>cycsch_methods_demo</callback>
		</demoitem>
	</demosection>
	
	<demosection>
		<label>Scheduling with Positive and Negative Time-Lags</label>
		<demoitem>
			<label>Scheduling with Positive and Negative Time-Lags algorithm</label>
			<callback>spntl_demo</callback>
		</demoitem>
	</demosection>
	
	<demosection>
		<label>SAT-based Scheduling</label>
		<demoitem>
			<label>Scheduling problem 'P|prec|Cmax'</label>
			<callback>sat_demo</callback>
		</demoitem>
	</demosection>
	
	<demosection>
		<label>Graph Algorithms</label>
		<demoitem>
			<label>Spaning tree</label>
			<callback>spanningtree_demo</callback>
		</demoitem>
		<demoitem>
			<label>Longest paths (Dijkstra's algorithm)</label>
			<callback>dijkstra_demo</callback>
		</demoitem>
		<demoitem>
			<label>Graph coloring 1</label>
			<callback>graphcoloring_demo</callback>
		</demoitem>
		<demoitem>
			<label>Graph coloring 2</label>
			<callback>graphcoloring_demo2</callback>
		</demoitem>
	</demosection>
	
	<demosection>
		<label>Integer Programming</label>
		<demoitem>
			<label>Integer Linear Programming</label>
			<callback>ilinprog_demo</callback>
		</demoitem>
		<demoitem>
			<label>Integer Quadratic Programming</label>
			<callback>iquadprog_demo</callback>
		</demoitem>
	</demosection>
	
</demos>
